{
    "name": "rizalfakhri/tgp-user-service-auth-sdk",
    "description": "The Authentication SDK for The Gym Pod Microservices kong-powered Architecture",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.5",
        "firebase/php-jwt": "^5.1",
        "phpseclib/phpseclib": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "TGP\\AuthServer\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Rizal Fakhri",
            "email": "me@rizalfakhri.id"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "TGP\\AuthServer\\Providers\\AuthServerServiceProvider"
            ],
            "aliases": {
                "AuthServer": "TGP"
            }
        }
    }
}
